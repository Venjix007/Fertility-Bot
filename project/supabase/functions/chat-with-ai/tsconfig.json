{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist",
    "strict": true,
    "target": "esnext",
    "skipLibCheck": true,
    "types": []
  },
  "include": ["./**/*.ts", "./deno.d.ts"],
  "exclude": ["node_modules"]
}
